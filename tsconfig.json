{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@env": [
        "./src/lib/env.js"
      ]
    },
    
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "docs",
    "cli",
    "android",
    "ios",
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts"
, "env.js"  ]
}
